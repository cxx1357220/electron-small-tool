{
  "name": "excel-json",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron": "cross-env NODE_ENV=master electron .",
    "test": "cross-env NODE_ENV=development electron .",
    "build:mac": "electron-builder --macos",
    "build:win": "electron-builder --win --x64"
  },
  "build": {
    "mac": {
      "icon": "./yang.png"
    },
    "win": {
      "icon": "./yang.png"
    }
  },
  "main": "src/electron.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "node-xlsx": "^0.15.0",
    "vue": "^2.6.10",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "electron": "^7.3.3",
    "electron-builder": "^22.8.1",
    "electron-prebuilt": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
